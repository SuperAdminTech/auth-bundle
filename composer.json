{
    "name": "superadmin/auth-bundle",
    "description": "SuperAdmin Bundle using ApiPlatform and JWT",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Lluis Santos",
            "email": "lluis@superadmin.org",
            "homepage": "https://github.com/santoslluis"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^2.2",
        "lexik/jwt-authentication-bundle": "^2.10",
        "symfony/framework-bundle": "^3.4|^4.0|^5.0",
        "symfony/security-bundle": "^3.4|^4.0|^5.0"
    },
    "require-dev": {
        "symfony/browser-kit": "^3.4|^4.0|^5.0",
        "symfony/console": "^3.4|^4.0|^5.0",
        "symfony/dom-crawler": "^3.4|^4.0|^5.0",
        "symfony/phpunit-bridge": "^3.4|^4.0|^5.0",
        "symfony/var-dumper": "^3.4|^4.0|^5.0",
        "symfony/yaml": "^3.4|^4.0|^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SuperAdmin\\Bundle\\": ""
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
